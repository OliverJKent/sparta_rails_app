<p id="notice"><%= notice %></p>

<h3>Account Sorts</h3>

<table>
  <thead>
    <tr>
      <th>Sort</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_sorts.each do |account_sort| %>
      <tr>
        <td><%= account_sort.sort %></td>
        <td><%= link_to 'Show', account_sort %></td>
        <td><%= link_to 'Edit', edit_account_sort_path(account_sort) %></td>
        <td><%= link_to 'Destroy', account_sort, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>



<div class= 'new'>
  <%= link_to 'New Account Sort', new_account_sort_path %>
</div>
